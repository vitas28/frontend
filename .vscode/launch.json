{
    "version": "0.2.1",
    "configurations": [
        {
            "type": "node",
            "request": "attach",
            "cwd": "${workspaceFolder}\\backend",
            "name": "Launch Program",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "outputCapture": "std",
            "resolveSourceMapLocations": [
                "${workspaceFolder}/**",
                "!**/node_modules/**"
            ],
            "processId": "${command:PickProcess}",
            "restart": true,
        }
    ]
}